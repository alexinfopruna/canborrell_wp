    .titol, .titol2{color:#444; font-size: 18px;}
    a{color: #666    }
    a:hover{color: #444}
    
    tr{height: 10px;}    
    table:first-child tr:first-child{background-color: white;}
    
    table{max-width: 700px;background-color: white;}
    body > table{width:100%;max-width: 700px;}  
